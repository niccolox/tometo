#!/usr/bin/env bash


if [[ "$*" == "vagrant" ]]
then
    npx concurrently -n "f,b" "npx webpack-dev-server --config ui/webpack.config.js --quiet --disable-host-check --host 0.0.0.0 --useLocalIp" "cd aph; mix phx.server"
else
    npx concurrently -n "f,b" "npx webpack-dev-server --config ui/webpack.config.js --quiet" "cd aph; mix phx.server"
fi
